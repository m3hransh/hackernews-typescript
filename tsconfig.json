{
  "$schema": "https://json.shemastore.org/tsconfig",
  "display": "Node 17",

  "compilerOptions" :{
    "lib": ["es2021"],
    "module": "commonjs",
    "target": "es2021",

    "strict": true,
    "outDir": "dist",
    "rootDir": "src",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include":["src/**/*"]
}
